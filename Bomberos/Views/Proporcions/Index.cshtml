@model IEnumerable<Bomberos.Models.Proporcion>

@{
    ViewData["Title"] = "Index";
}

<h1>Proporciones</h1>

<p>
    <a asp-action="Create">Crear Nuevo Registro</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @*@Html.DisplayNameFor(model => model.PTipoProp)*@
                Nombre Proporción
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PTipoProp)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdProp">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.IdProp">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.IdProp">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
