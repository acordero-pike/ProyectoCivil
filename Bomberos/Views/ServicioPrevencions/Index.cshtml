                @Html.DisplayFor(modelItem => item.SpTurnoNavigation.IdTurno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SpVoBoJefeServicioNavigation.IdFirma)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdSp">Editar</a> 
                <a asp-action="Details" asp-route-id="@item.IdSp">Detalles</a> 
                <a asp-action="Delete" asp-route-id="@item.IdSp">Eliminar</a>
            </td>
        </tr>
}
