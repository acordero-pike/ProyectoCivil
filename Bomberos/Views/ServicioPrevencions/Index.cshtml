@model IEnumerable<Bomberos.Models.ServicioPrevencion>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<h1>Servicio de Prevención</h1>

<p>
    <a asp-action="Create">Crear Nuevo Registro</a>
</p>
<div style="overflow-x:auto;">
<table class="table">
    <thead>
        <tr>
            <th>
                @*@Html.DisplayNameFor(model => model.SpDireccion)*@
                Dirección
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.SpNombreRazonsocial)*@
                Nombre de Razon Social
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.SpGeneralesServPrestado)*@
                Servicios Generales Prestados
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.SpEstablecimiento)*@
                Establecimiento
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.SpHoraSalida)*@
                Hora de Salida
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.SpHoraEntrada)*@
                Hora de Entrada 
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.SpUnidad)*@
                Unidad
            </th>
            <th>
